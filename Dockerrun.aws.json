{
"AWSEBDockerrunVersion": 2,
"containerDefinitions": [
{
    "name": "client",
    "image": "victorkulcsar/multi-client",
    "hostname": "client",
    "essential": false,
    "memory": 128
},
{
    "name": "server",
    "image": "victorkulcsar/multi-server",
    "hostname": "api",
    "essential": false,
    "memory": 128
},
{
    "name": "worker",
    "image": "victorkulcsar/multi-worker",
    "hostname": "worker",
    "essential": false,
    "memory": 128
},
{
    "name": "nginx",
    "image": "victorkulcsar/multi-nginx",
    "hostname": "nginx",
    "essential": true,
    "portMappings": [
        {
        "hostPort": 80,
        "containerPort": 80
        }
    ],
    "links": ["client", "server"],
    "memory": 128
}
]

}